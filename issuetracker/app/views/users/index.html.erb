<p style="color: green"><%= notice %></p>
    <body>
    <h1>User List </h1>
    <form action="<%= @ticket %>" style='margin-left:800px;'>
      <input type="text" style='border: lightblue 3px outset;'  placeholder="User" name="search_keyU">
      <button type="submit" class="btn btn-info btn-m" >Search</button>
    </form>
    <div>
<button type="button" class="btn btn-outline-info" ><%= link_to "Back", root_path %> 
</div>
    <table class="table">
  <thead class="bg-info">
      <th>ID</th>
      <th>Name</th>
      <th>Last Name</th>
      <th>Roll</th>
      <th>View</th>
      <th>Edit</th>
      </thead>
    <tbody>
    <% @users.each do |user| %>
    <%= render user %>
        <tr>
          <td><%=user.id%></td>
          <td><%=user.first_name%></td>
          <td><%=user.last_name%></td>
          <td><%=user.roll.roll%></td>
          
              <td><button type="button" class="btn btn-sm btn-outline-secondary"><%= link_to "View", user %></button></td>
              <td><button type="button" class="btn btn-sm btn-outline-secondary"><%= link_to "Edit", edit_user_path(user) %></button></td>
  
        <tr>
    </tbody>
</p>
  <% end %>

